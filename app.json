{
  "expo": {
    "name": "SafeStreets Bangladesh",
    "slug": "safestreets-bangladesh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safestreets",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser", 
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SafeStreets to use your location to report incidents accurately."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}